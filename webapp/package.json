    "@types/react-infinite-scroller": "^1.2.5",
    "@types/react-modal": "^3.16.3",
    "@vitejs/plugin-legacy": "^7.1.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "stylelint": "^16.20.0",
    "stylelint-config-prettier-scss": "^1.0.0",
