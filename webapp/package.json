{
  "name": "@pets/webapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "dotenv -e .env.test -e .env jest",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.app.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts --ext .tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.scss",
    "stylecheck": "sass -q --load-path=node_modules ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --loglevel warn --cache --write \"{src/**/*,*}.{ts,tsx,js,json,yml,scss}\""
  },
  "dependencies": {
    "@pets/backend": "workspace:*",
    "@pets/shared": "workspace:*",
