    "react-dom": "^19.1.0",
    "react-head": "^3.4.2",
    "react-icons": "^5.5.0",
    "react-image-gallery": "^1.4.0",
    "react-infinite-scroller": "^1.2.6",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.6.1",
    "react-simplemde-editor": "^5.2.0",
    "reset-css": "^5.0.2",
    "sass": "^1.89.2",
