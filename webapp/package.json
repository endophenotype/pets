    "@sentry/react": "7.114.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "10.37.0",
    "@trpc/react-query": "10.37.0",
    "@trpc/server": "10.37.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "easymde": "^2.20.0",
    "formik": "^2.4.6",
